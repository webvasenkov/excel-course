{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "semi": "off",
    "object-curly-spacing": "off",
    "linebreak-style": "off",
    "indent": "off",
    "comma-dangle": "off"
  }
}